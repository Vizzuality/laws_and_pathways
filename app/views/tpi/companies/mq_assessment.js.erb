(function () {
  document.getElementById('mq-assessment').innerHTML = "<%= j render('mq_assessment', assessment: @mq_assessment) %>";
  document.getElementById('mq-assessment-metadata').innerHTML = "<%= j render('mq_assessment_metadata', assessment: @mq_assessment) %>";
  const newUrl = new URL(window.location.href);
  newUrl.searchParams.set('mq_assessment_id', <%= @mq_assessment.id %>);
  window.history.replaceState({}, '', newUrl.href);
  ReactRailsUJS.mountComponents('#mq-assessment');
})();
